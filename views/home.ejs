<!-- AI Automation Arsenal -->
<div class="ai-agents-section">
    <div class="container">
        <h2>Your AI Automation Arsenal</h2>
        <p class="section-subtitle">Meet the intelligent agents that will transform your workflow</p>
        <div class="floating-agents">
        <div class="agent-card premium" data-agent="linkedin">
            <div class="agent-header">
                <div class="agent-avatar">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"/>
