<!-- Working Terminal Animation -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    const terminalOutput = document.getElementById('terminalOutput');
    
    if (!terminalOutput) {
        console.error('Terminal output not found');
        return;
    }
    
    const messages = [
        { user: 'User$', text: 'I need to scrape 500 LinkedIn profiles from tech companies', color: '#00ff00' },
        { user: 'AI-Agent$', text: 'Processing LinkedIn scraping request... ‚úì', color: '#ff6b35' },
        { user: 'User$', text: 'Can you enrich these contacts with email addresses?', color: '#00ff00' },
        { user: 'AI-Agent$', text: 'Initiating contact enrichment with 95% accuracy... ‚úì', color: '#ff6b35' },
        { user: 'User$', text: 'Set up automated follow-up emails for my prospects', color: '#00ff00' },
        { user: 'AI-Agent$', text: 'Email campaign sequence configured successfully ‚úì', color: '#ff6b35' }
    ];
    
    let currentMessageIndex = 0;
    
    function typeMessage(message, callback) {
        const line = document.createElement('div');
        line.style.marginBottom = '10px';
        line.style.fontFamily = 'Monaco, "Courier New", monospace';
        
        const prompt = document.createElement('span');
        prompt.textContent = message.user + ' ';
        prompt.style.color = message.color;
        prompt.style.fontWeight = 'bold';
        
        const textSpan = document.createElement('span');
        textSpan.style.color = '#ffffff';
        
        const cursor = document.createElement('span');
        cursor.textContent = '_';
        cursor.style.color = message.color;
        cursor.style.animation = 'blink 1s infinite';
        
        line.appendChild(prompt);
        line.appendChild(textSpan);
        line.appendChild(cursor);
        terminalOutput.appendChild(line);
        
        let charIndex = 0;
        const text = message.text;
        
        function typeChar() {
            if (charIndex < text.length) {
                textSpan.textContent += text.charAt(charIndex);
                charIndex++;
                setTimeout(typeChar, 50 + Math.random() * 50);
            } else {
                cursor.remove();
                setTimeout(callback, 1500);
            }
        }
        
        typeChar();
    }
    
    function typeNextMessage() {
        if (currentMessageIndex >= messages.length) {
            setTimeout(() => {
                terminalOutput.innerHTML = '';
                currentMessageIndex = 0;
                typeNextMessage();
            }, 3000);
            return;
        }
        
        typeMessage(messages[currentMessageIndex], () => {
            currentMessageIndex++;
            typeNextMessage();
        });
    }
    
    // Start typing animation
    setTimeout(typeNextMessage, 1000);
});
</script>



