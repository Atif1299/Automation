<%- include('partials/header') %>

<!-- Particle Animation Background -->
<div class="particles-container">
    <canvas id="particlesCanvas"></canvas>
</div>

<div class="hero particles-bg">
    <h1>Automation and Enrichment </h1>
    <p>Streamline your LinkedIn & Email outreach and enrich your contact lists with our powerful ai automations</p>
    
    <a href="/auth/client-login" class="cta-button morph-button ripple">
        Get Started
        <span>→</span>
    </a>
</div>


<!-- AI Workflow Process -->
<div class="workflow-section">
    <div class="container">
        <h2>How Your AI Agents Work Together</h2>
        <div class="workflow-steps">
            <div class="workflow-step">
                <div class="step-number">01</div>
                <div class="step-icon">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"/>
                    </svg>
                </div>
                <h3>Target Identification</h3>
                <p>AI agents analyze your requirements and identify the perfect prospects from LinkedIn</p>
            </div>
            <div class="workflow-arrow">→</div>
            <div class="workflow-step">
                <div class="step-number">02</div>
                <div class="step-icon">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                    </svg>
                </div>
                <h3>Intelligent Processing</h3>
                <p>Advanced algorithms enrich contact data with valuable insights and business intelligence</p>
            </div>
            <div class="workflow-arrow">→</div>
            <div class="workflow-step">
                <div class="step-number">03</div>
                <div class="step-icon">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2L3 5v6c0 5.55 3.84 10.74 9 12 5.16-1.26 9-6.45 9-12V5l-9-4z"/>
                    </svg>
                </div>
                <h3>Automated Outreach</h3>
                <p>Personalized campaigns are launched with AI-optimized messaging and timing</p>
            </div>
        </div>
    </div>
</div>


<!-- Interactive Terminal -->
<div class="terminal-section">
    <div class="terminal-window">
        <div class="terminal-header">
            <div class="terminal-controls">
                <span class="control close"></span>
                <span class="control minimize"></span>
                <span class="control maximize"></span>
            </div>
            <div class="terminal-title">AI Agent Console</div>
        </div>
        <div class="terminal-body">
            <div id="terminalOutput">
                <!-- Dynamic content will be generated here -->
            </div>
        </div>
    </div>
</div>

    </div>
</div>

